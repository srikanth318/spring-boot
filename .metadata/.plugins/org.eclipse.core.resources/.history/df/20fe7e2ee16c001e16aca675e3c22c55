package com.interview.questions;

import java.util.Arrays;
import java.util.Comparator;

public class ExTest {
	
	public static void main(String[] args) {
		int [] arr ={1,4,5,3,2,6,6,7,7};
		Integer integer = Arrays.stream(arr).boxed().distinct().sorted(Comparator.reverseOrder()).skip(1).findFirst().get();
		
		System.out.println(integer);
		
	}
}


