package com.test;

import java.util.Arrays;
import java.util.List;
import java.util.Map;
import java.util.Map.Entry;
import java.util.stream.Collectors;

public class Test {

	public static void main(String[] args) {
		
		int num = 1232333;
		
		String[] split = String.valueOf(num).split("");
		
		List<String> list = Arrays.asList(split);
	System.out.println(list);
		Map<String, Long> collect = list.stream().collect(Collectors.groupingBy(s->s,Collectors.counting()));
		
		Entry<String, Long> entry = collect.entrySet().stream().max(Map.Entry.comparingByValue()).get();

		System.out.println("Higest repeating value is "+entry.getKey()+" --- "+entry.getValue());
 
	}

}
