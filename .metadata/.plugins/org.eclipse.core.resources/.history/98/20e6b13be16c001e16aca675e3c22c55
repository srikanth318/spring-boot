package com.interview.questions;

import java.util.Arrays;
import java.util.Comparator;

public class ExTest {
	public static int secondNumber(int arr[],int total) {
		Arrays.sort(arr);
	
	return arr[total-2];
	}
	public static void main(String[] args) {
		int [] arr ={1,4,5,3,2,6,6,7,7};
		System.out.println("the second largest number is :" +secondNumber(arr,9));
		
	}
}


