package com.demointerview.dto;

import com.demointerview.pojo.MovieDetails;
import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

import java.io.Serializable;
import java.util.List;

@NoArgsConstructor
@AllArgsConstructor
@Builder
@Data
public class DirectorDetails implements Serializable {

    private static final long serialVersionUID = 634961036400971080L;
    private String name;
    private List<MovieDetails> movieDets;
}
